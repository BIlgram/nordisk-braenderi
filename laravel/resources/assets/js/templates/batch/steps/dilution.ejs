<div class="headings">
    <h2>Dilution</h2>
</div>
<form>
    <label>
        <span>Fortyndingsdato</span>
        <input type="date">
    </label>
    <label>
        <span>Kommentarer</span>
        <textarea rows="5"></textarea>
    </label>
    <h3>Inden fortynding</h3>
    <label>
        <span>Totalvægt(kg)</span>
        <input type="number" step="0.01">
    </label>
    <label>
        <span>Alkohol(%)</span>
        <input type="number" step="0.01">
    </label>
    <label>
        <span>Tabelværdi for alkohol</span>
        <input type="number" step="0.01">
    </label>
    <label>
        <span>Ønsket alkohol(%)</span>
        <input type="number" step="0.01">
    </label>
    <label>
        <span>Tabelværdi for ønsket alkohol</span>
        <input type="number" step="0.01">
    </label>
    <h3>Efter fortyndingen</h3>
    <div class="output">
        <span>100% ren alkohol(kg)</span>
        <div>12312321</div>
    </div>
    <div class="output">
        <span>Tilsætning af vand</span>
        <div>321321123</div>
    </div>
    <div class="output">
        <span>Tilsætning af vand(kg)</span>
        <div>123123123321</div>
    </div>
    <button id="save-dilution">Gem fortynding</button>
</form>
